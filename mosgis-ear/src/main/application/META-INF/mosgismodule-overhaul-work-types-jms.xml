   <sub-deployment-name>MosGisJmsModuleSubbb</sub-deployment-name>
   <jndi-name>mosgis.inExportOverhaulWorkTypesQueue</jndi-name>
   <delivery-params-overrides>
       <time-to-deliver>2000</time-to-deliver>
       <redelivery-delay>10000</redelivery-delay>
   </delivery-params-overrides>
 </uniform-distributed-queue>
 
 <!-- очередь опроса статуса исходящих запросов на экспорт справочника видов работ капитального ремонта -->
 <uniform-distributed-queue name="outExportOverhaulWorkTypesQueue">
   <sub-deployment-name>MosGisJmsModuleSubbb</sub-deployment-name>    
   <jndi-name>mosgis.outExportOverhaulWorkTypesQueue</jndi-name>
   <delivery-params-overrides>
       <time-to-deliver>2000</time-to-deliver>
       <redelivery-delay>10000</redelivery-delay>
   </delivery-params-overrides>
 </uniform-distributed-queue>

</weblogic-jms>
