<?xml version='1.0' encoding='UTF-8'?>
<weblogic-jms xmlns="http://xmlns.oracle.com/weblogic/weblogic-jms" xmlns:sec="http://xmlns.oracle.com/weblogic/security" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:wls="http://xmlns.oracle.com/weblogic/security/wls" xsi:schemaLocation="http://xmlns.oracle.com/weblogic/weblogic-jms http://xmlns.oracle.com/weblogic/weblogic-jms/1.1/weblogic-jms.xsd">
    
 <!-- очередь обработки запросов на импорт справочника видов работ капитального ремонта -->
 <uniform-distributed-queue name="inImportOverhaulWorkTypesQueue">
   <sub-deployment-name>MosGisJmsModuleSubbb</sub-deployment-name>
   <jndi-name>mosgis.inImportOverhaulWorkTypesQueue</jndi-name>
   <delivery-params-overrides>
       <time-to-deliver>2000</time-to-deliver>
       <redelivery-delay>10000</redelivery-delay>
   </delivery-params-overrides>
 </uniform-distributed-queue>
 
 <!-- очередь опроса статуса исходящих запросов на импорт справочника видов работ капитального ремонта -->
 <uniform-distributed-queue name="outImportOverhaulWorkTypesQueue">
   <sub-deployment-name>MosGisJmsModuleSubbb</sub-deployment-name>    
   <jndi-name>mosgis.outImportOverhaulWorkTypesQueue</jndi-name>
   <delivery-params-overrides>
       <time-to-deliver>2000</time-to-deliver>
       <redelivery-delay>10000</redelivery-delay>
   </delivery-params-overrides>
 </uniform-distributed-queue>
 
 <!-- очередь обработки запросов на экспорт справочника видов работ капитального ремонта -->
