							<xs:sequence>
								<xs:element ref="tns:InsuranceProductGUID"/>
								<xs:element name="Rate">
									<xs:annotation>
										<xs:documentation>Тариф
Ссылка на пост. 924 – Приложение 2, п. 17.</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:decimal">
											<xs:fractionDigits value="6"/>
											<xs:totalDigits value="14"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="TotalPayable">
									<xs:annotation>
										<xs:documentation>Итого к оплате за расчетный период, руб.
Ссылка на пост. 924 – Приложение 2, п. 19.</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="bills-base:MoneyType">
											<xs:totalDigits value="13"/>
										</xs:restriction>
									</xs:simpleType>
